<%- include ("layouts/header.ejs") %>
 <div class="col-md-12">
    <% if(msg != ""){ %>
        <div class="alert alert-success" role="alert">
            <%- msg %>
        </div>
    <% } %>
    <table class="table table-striped custab">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Active</th>
                <th class="text-center">Action</th>
            </tr>
        </thead>
            <% devices.forEach(function(device){ %>
               <% if (typeof device != "undefined") {%>
                <tr>
                    <td><%- device.device_id %></td>
                    <td><%- device.device_name %></td>
                    <td><%- device.active %></td>
                    <td class="text-center"><a class='btn btn-info btn-xs' href="/admin/devices/edit/<%- device.device_id %>"><span class="glyphicon glyphicon-edit"></span> Edit</a> <a href="/admin/devices/del/<%- device.device_id %>" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> Del</a></td>
                </tr>
                <% } %>
            <% }) %>
        </table>
 </div>
<%- include ("layouts/footer.ejs") %>